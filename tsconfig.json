{
  "baseUrl": ".",
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  },
  "paths": {
    "@components/*": ["src/components/*"],
    "@layouts/*": ["src/layouts/*"],
    "@solid/*": ["src/layouts/*"],
    "@utils/*": ["src/utils/*"],
    "@common/*": ["src/common/*"]
  }
}
